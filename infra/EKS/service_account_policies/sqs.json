{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "sqs:*"
            ],
            "Effect": "Allow",
            "Resource": "*"
        },
        {
          "Effect" : "Allow",
          "Action" : [
            "s3:*"
          ],
          "Resource" : [
            "*"
          ]
        },
        {
          "Effect" : "Allow",
          "Action" : [
            "ses:*"
          ],
          "Resource" : [
            "*"
          ]
        },
        {

          "Effect" : "Allow",
          "Action" : ["dynamodb:*"],
          "Resource" : "arn:aws:dynamodb:::table/${dynamodb_table_name}"
        }
    ]
}